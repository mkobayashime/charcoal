{
  "version": "independent",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "changelogPreset": {
    "name": "conventionalcommits"
  },
  "command": {
    "version": {
      "message": "chore: publish [skip ci]"
    },
    "publish": {
      "verifyAccess": false,
      "conventionalCommits": true,
      "changelog": false,
      "graphType": "all"
    },
    "run": {
      "stream": true,
      "loglevel": "success"
    }
  }
}
